extends layout
block content
   .container
      nav.navbar.navbar-default
         .container-fluid
            .navbar-header
               a.navbar-brand EvoSpace Dashboard
      .row
        .col-sm-8
           .panel.panel-default
              .panel-heading
                 .panel-title Population
              #population.panel-body
           button#evolve.btn.btn-default.btn-block(type="button") Evolve


        .col-sm-3
           .panel.panel-default
              .panel-heading
                 .panel-title Sample Queue
              #sample-queue.panel-body
           button#respawn.btn.btn-default.btn-block(type="button") Respawn

block scripts
   script(src="/javascripts/jquery.min.js")
   script(src="/javascripts/evospace-dashboard.js")
